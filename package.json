{
  "name": "ts-angularjs-webpack-starter",
  "version": "0.0.1",
  "private": true,
  "main": "src/server/main.ts",
  "scripts": {
    "node-pg-migrate": " node-pg-migrate -t _migrations",
    "nodemon": "nodemon --exec ts-node --inspect . --ext 'ts' --watch 'src/server'",
    "start": "concurrently --raw \"npm run webpack\" \"npm run nodemon\"",
    "test": "jest",
    "webpack": "webpack-dev-server -d"
  },
  "dependencies": {
    "@angular/common": "~5.0.0",
    "@angular/compiler": "~5.0.0",
    "@angular/core": "~5.0.0",
    "@angular/forms": "~5.0.0",
    "@angular/platform-browser": "~5.0.0",
    "@angular/platform-browser-dynamic": "~5.0.0",
    "@angular/router": "~5.0.0",
    "bootstrap-css-only": "~4.0.0-beta",
    "bson": "~1.0.0",
    "express": "~4.16.0",
    "jsbn": "~1.1.0",
    "mongo-sql": "~4.0.0",
    "pg": "~7.4.0",
    "reflect-metadata": "~0.1.0",
    "rxjs": "~5.5.0",
    "zone.js": "~0.8.0"
  },
  "devDependencies": {
    "@types/bson": "~1.0.0",
    "@types/express": "~4.0.0",
    "@types/jest": "~21.1.0",
    "@types/jsbn": "~1.2.0",
    "@types/node": "^8.0.58",
    "@types/pg": "~7.4.0",
    "angular2-template-loader": "~0.6.0",
    "concurrently": "~3.5.0",
    "css-loader": "~0.28.0",
    "file-loader": "~1.1.0",
    "jest": "~21.2.0",
    "node-pg-migrate": "~2.14.0",
    "nodemon": "~1.12.0",
    "raw-loader": "~0.5.0",
    "style-loader": "~0.19.0",
    "ts-jest": "~21.2.0",
    "ts-loader": "~3.1.0",
    "ts-node": "~3.3.0",
    "tslint": "~5.8.0",
    "typescript": "~2.6.0",
    "webpack": "~3.8.0",
    "webpack-dev-server": "~2.9.0"
  },
  "jest": {
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  }
}
